{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "security:minimumReleaseAgeNpm",
    "schedule:daily"
  ],
  "automerge": true,
  "automergeType": "branch",
  "packageRules": [
    {
      "matchManagers": [
        "npm"
      ],
      "matchDepTypes": [
        "devDependencies"
      ],
      "enabled": false
    }
  ]
}
